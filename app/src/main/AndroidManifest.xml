<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="am.ze.wookoo.withtaxi">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application


        android:allowBackup="true"
        android:icon="@drawable/ic_taxi_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_taxi_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-4210582747176451~9351818945"/>
        <activity android:name=".ReadyToMatching"></activity>
        <activity android:name=".Matching" />
        <activity android:name=".StartToElse" />
        <activity android:name=".StartToStation" />
        <activity android:name=".StartPinActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>